<%# New Meetup - Redesigned %>
<nav class="breadcrumbs" aria-label="Nawigacja">
  <%= link_to "Dashboard", admin_root_path %>
  <span class="breadcrumbs-separator">/</span>
  <%= link_to "Spotkania", admin_meetups_path %>
  <span class="breadcrumbs-separator">/</span>
  <span class="breadcrumbs-current">Nowe spotkanie</span>
</nav>

<div class="page-header">
  <h1 class="page-title">Nowe spotkanie</h1>
</div>

<div class="card">
  <div class="card-body">
    <%= form_with model: [:admin, @meetup], local: true, class: "form" do |f| %>
      <%= render "form", meetup: @meetup, f: f %>
    <% end %>
  </div>
</div>
